<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="struts-starter" extends="struts-default">
		<result-types>
			<result-type name="tiles"
				class="org.apache.struts2.views.tiles.TilesResult" />
		</result-types>
		<action name="hello" class="tutorial.HelloWorld">
			<result name="success">/WEB-INF/views/HelloWorld.jsp</result>
		</action>
		<!-- Add your actions here -->
		<action name="welcome" class="tutorial.Welcome">
			<result name="success">/WEB-INF/views/welcome.jsp</result>
		</action>
	</package>
</struts>